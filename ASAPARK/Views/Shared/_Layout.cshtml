<!DOCTYPE html>
<html lang="pt" xmlns="http://www.w3.org/1999/html">
<head>
    <title>ESTACIONAMENTO</title>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />

    <!-- Bootstrap core CSS -->
    <link href="~/Content/webjars.css" rel="stylesheet" />
    <link href="~/lib/font-awesome/css/all.css" rel="stylesheet" />



    <!-- Custom styles for this template -->
    <link href="~/Content/style.css" rel="stylesheet" />

    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.19/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/responsive/2.2.3/css/responsive.bootstrap4.min.css">

    
</head>
<body>

    <header>
        <nav class="navbar navbar-inverse navbar navbar-dark bg-dark bg-company fixed-top">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="btn btn-dark navbar-toggle bg-company2 pull-left">
                        <i class="fas fa-bars sizebtnh"></i>
                    </button>

                    <img src="~/lib/Imagens/AsaPark.jpg" width="60" height="40" class="img-responsive">
                    <a class="navbar-brand " href="#">ESTACIONAMENTO</a>
                </div>
                <div class="nav nav-pills">
                    <a class="nav-link2 sizet" href="/Funcionarios/Funcionarios"> <i class="fas fa-users sizet"></i> <span>Funcionários</span></a>
                </div>
                <div class="nav nav-pills">
                    <a class="nav-link2 sizet" href="/MeusDados/MeusDados"> <i class="fas fa-user-circle sizet"></i> <span>Meus Dados</span></a>
                </div>
                <div class="nav nav-pills">
                    <a class="nav-link2 sizet" href="/"> <i class="fas fa-power-off sizet"></i> <span>Sair</span></a>
                </div>
            </div>
        </nav>
    </header>

    <div class="layout-main">

        <aside>
            <nav class="sidebar sidebar-open">
                <ul class="nav nav-pills">
                    <li class="nav-item">
                        <a class="nav-link " href="/Home/Index">
                            <i class="fas fa-home"></i> <span>Home</span>
                        </a>
                    </li>
                </ul>

                <ul class="nav nav-pills">
                    <li class="nav-item"><span class="nav-link active">ENTRADA</span></li>
                    <li class="nav-item">
                        <a class="nav-link" href="/EntraSaida/CadastroEntrada">
                            <i class="fa fa-plus-circle"></i> <span>Entrada de Veiculos</span>
                        </a>
                    <li class="nav-item">
                        <a class="nav-link" href="/EntraSaida/ReimpressaoEntrada">
                            <i class="fa fa-print"></i> <span>Reimpressão Ticket Entrada</span>
                        </a>
                    </li>
                    </li>

                </ul>


                <ul class="nav nav-pills">
                    <li class="nav-item"><span class="nav-link active">SAIDA</span></li>                    
                    <li class="nav-item">
                        <a class="nav-link" href="/Saida/ConsultarSaida">
                            <i class="fas fa-search"></i> <span> Todas Entrada </span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Saida/CosultaPordataAtual">
                            <i class="fa fa-car"></i> <span>Saída dia - @DateTime.Now.ToString("dd/MM/yy")</span>
                        </a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" href="/Saida/Impressao">
                            <i class="fa fa-print"></i> <span>Impressão Ticket Saída</span>
                        </a>
                    </li>
                </ul>


                <ul class="nav nav-pills">
                    <li class="nav-item"><span class="nav-link active">ACESSO ADMIN</span></li>
                    <li class="nav-item">
                        
                        <a class="nav-link" href="/Admin/Index">
                            <i class="fas fa-plus-circle"></i> <span>Admin</span>
                        </a>
                    </li>

                </ul>

            </nav>
        </aside>


        <!-- Bootstrap core JavaScript
    ================================================== -->


        <script src="~/Scripts/jquery.js"></script>
        <script src="~/Scripts/jquery-mask.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"></script>
        <script src="~/Scripts/js-bootstrap.js"></script>
        <script src="~/Scripts/jquery.moneymask.js"></script>
        

        <script type="text/javascript">
            $(function () {
                $('[data-toggle="popover"]').popover();
            });

            $(document).ready(function () {
                $(".navbar-toggle").click(function () {
                    $(".sidebar").toggleClass("sidebar-open");
                })
            });

        </script>

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)
        <script src="~/Scripts/jquery.js"></script>
        <script src="~/Scripts/jquery-mask.js"></script>
        <script src="~/Scripts/js-bootstrap.js"></script>
        @Scripts.Render("~/bundles/inputmask")
        @section Scripts {
            @Scripts.Render("~/bundles/jqueryval")
        }
        @RenderBody()
       <script src="~/Scripts/jquery.moneymask.js"></script>



</body>
</html>
